[user]
	email = kaityo@users.sourceforge.jp
	name = H. Watanabe
[color]
	ui = true
[alias]
	st = status -s
	l = log --oneline
[core]
	whitespace = cr-at-eol
	excludesFile = ~/.gitignore_global
	editor = vim
[push]
	default = simple
[fetch]
	prune = true
[pull]
	ff = only
[init]
	defaultBranch = main
